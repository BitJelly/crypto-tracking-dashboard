  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:justinformentin/crypto-tracking-dashboard"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "moment": "^2.19.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.4.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-select": "^1.2.1",
    "react-test-renderer": "^16.4.1",
    "react-twitter-widgets": "^1.7.1",
    "reactstrap": "^6.1.0",
    "recharts": "^1.0.0-beta.10"
  },
  "devDependencies": {
    "babel-jest": "^23.0.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "bugs": {
    "url": "https://github.com/justinformentin/crypto-tracking-dashboard/issues"
  }
}
